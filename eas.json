{
  "cli": {
    "version": ">= 7.0.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal"
    },
    "production": {
      "autoIncrement": true,
      "env": {
        "FOOTBALL_API_KEY": "$FOOTBALL_API_KEY",
        "FOOTBALL_API_BASE_URL": "$FOOTBALL_API_BASE_URL"
      }
    }
  },
  "submit": {
    "production": {
      "ios": {
        "appleId": "$EXPO_APPLE_ID",
        "ascAppId": "$EXPO_APP_ID",
        "appleTeamId": "$EXPO_TEAM_ID"
      },
      "android": {
        "serviceAccountKeyPath": "pc-api-key.json",
        "track": "production"
      }
    }
  }
}
